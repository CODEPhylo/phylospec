moves = VectorMoves()
monitors = VectorMonitors()

alignment := readDiscreteCharacterData( file="hallo.nex" )
a ~ dnCoalescent( theta=200, taxa=alignment.taxa() )
moves.append( mvNarrow( a ) )
moves.append( mvNNI( a ) )
moves.append( mvFNPR( a ) )
moves.append( mvSubtreeScale( a ) )
moves.append( mvNodeTimeSlideUniform( a ) )
moves.append( mvRootTimeScaleBactrian( a ) )
moves.append( mvTreeScale( a ) )
taxa := a.taxa()
b := taxa.size()
temp_taxa := taxa
for (i in 1:temp_taxa.size()) {
temp_names[i] := temp_taxa[i].getName()
}
c := temp_names

monitors.append( mnModel( filename = "taxonset.log", printgen = 10 ) )
monitors.append( mnFile( filename = "taxonset.trees", printgen = 10 ) )
monitors.append( mnScreen( printgen = 10 ) )

mymodel = model( a )
mymcmc = mcmc( mymodel, monitors, moves )
mymcmc.run( generations=20000, tuningInterval=200 )

q()